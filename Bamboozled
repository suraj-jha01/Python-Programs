s= "Bamboozled"
#extract B a
print(s[0],s[1])
#extract e d
print(s[8],s[9])
print(s[8],s[9])
#extract mboozled
print(s[2:10])
print(s[2:])
print(s[2:10])
# extract Bamboo
print(s[0:6])
print(s[0:6])
print(s[0:6])
print(s[0:6])
# reverse bamboozled
print(s[::-1])
# etract Bamboozled
print(s)
# extract 
print(s[0:11:2])
print(s[0:10:3])
print(s[0:11:4])
s=s+'hype!'
print(s)
s=s[:6]+'Monger'+ s[-1]
